{
  "name": "solid-form-handler",
  "description": "A SolidJS library for simplifying form validations.",
  "version": "1.0.0-alpha.7",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "SolidJS forms",
    "Form validation libraries",
    "Form components"
  ],
  "author": {
    "name": "Mauricio Rivera"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webblocksapp/solid-form-handler"
  },
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.es.js",
  "main": "./dist/index.cjs.js",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "node": "./dist/index.cjs.js"
    }
  },
  "scripts": {
    "build": "vite build --config=vite.prod.ts && tsc-alias && yarn copy",
    "copy": "cp ../../README.md .",
    "start": "vite --config=vite.dev.ts",
    "test": "jest --coverage"
  },
  "peerDependencies": {
    "solid-js": "^1"
  },
