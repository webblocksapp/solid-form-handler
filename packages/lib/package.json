    "@babel/preset-typescript": "^7.16.7",
    "@suid/material": "^0.6.3",
    "@types/jest": "^27.5.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.0",
    "babel-loader": "^8.2.5",
